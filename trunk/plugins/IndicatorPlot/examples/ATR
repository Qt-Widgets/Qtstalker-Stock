[symbol]
plugin=Symbol
exchange=
ticker=

[plot]
plugin=Plot
grid_x=true
grid_y=true
info=true
date=true
row=1
col=1

[atr]
plugin=ATR
input_object=symbol
high_key=H
low_key=L
close_key=C
period=14

[plot%20atr]
plugin=CurveLine
width=1
input_object=atr
input_key=v
label=ATR
style=Line
color=@Variant(\0\0\0\x43\x1\xff\xff\xff\xff\xff\xff\0\0\0\0)
plot_object=plot

[General]
order=symbol, plot, atr, plot atr
