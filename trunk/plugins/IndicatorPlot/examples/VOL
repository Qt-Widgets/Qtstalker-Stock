[symbol]
plugin=Symbol
exchange=
ticker=

[plot]
plugin=Plot
grid_x=true
grid_y=true
info=true
date=true
row=1
col=1

[plot%20volume]
plugin=CurveBar
input_object=symbol
input_key=V
label=V
color=@Variant(\0\0\0\x43\x1\xff\xff\0\0\0\0\xff\xff\0\0)
plot_object=plot

[compare%20up%20bars]
plugin=CompareValues
input_object=symbol
input2_object=symbol
input_key=C
input2_key=C
op=5
offset=0
offset2=1

[compare%20down%20bars]
plugin=CompareValues
input_object=symbol
input2_object=symbol
input_key=C
input2_key=C
op=0
offset=0
offset2=1

[color%20up%20bars]
plugin=CurveColor
input_object=compare up bars
input2_object=plot volume
input_key=R
op=2
color=@Variant(\0\0\0\x43\x1\xff\xff\0\0\xff\xff\0\0\0\0)
value=1
plot_object=plot

[color%20down%20bars]
plugin=CurveColor
input_object=compare down bars
input2_object=plot volume
input_key=R
op=2
color=@Variant(\0\0\0\x43\x1\xff\xff\xff\xff\0\0\0\0\0\0)
value=1
plot_object=plot

[ma1]
plugin=MA
input_object=symbol
input_key=V
type=EMA
period=14

[plot%20ma1]
plugin=CurveLine
width=1
input_object=ma1
input_key=MA
label=MA1
style=Line
color=@Variant(\0\0\0\x43\x1\xff\xff\xff\xff\xff\xff\0\0\0\0)
plot_object=plot

[General]
order=symbol, plot, plot volume, compare up bars, compare down bars, color up bars, color down bars, ma1, plot ma1
